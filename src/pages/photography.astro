---
import Layout from "../layouts/Layout.astro";
import PageHeader from "../components/PageHeader.astro";
import SectionIntro from "../components/SectionIntro.astro";
import PageContainer from "../components/PageContainer.astro";

const title = "Photography";
const currentPage = "photography";
---

<Layout {title} {currentPage}>
<PageHeader pageNumber="10" heading="Photography" />
  <PageContainer>
   <SectionIntro
    text="We would love for you to capture great group and running photos. Here are some tips to help ensure your photos look their best."
  />
</PageContainer>
</Layout>